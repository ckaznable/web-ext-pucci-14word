  "browser_action": {
    "default_icon": "icons/icon-128.png"
  },
  "content_security_policy":"script-src 'self'; object-src 'self';",
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "manifest_version": 2,
  "web_accessible_resources": [
    "style/*"
  ]
}