{
  "background": {
    "service_worker": "script/background.js"
  },
  "content_security_policy": {
    "script-src": "self",
    "object-src": "self"
  },
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_title": "Click to show an alert"
  },
  "manifest_version": 3,
  "web_accessible_resources": [
    {
      "resources": [
        "style/*"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}